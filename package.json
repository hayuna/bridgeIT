{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run client:build && pwd && npm run clean && npm run backend:build && node dist/index.js",
    "dev": "NODE_ENV=development babel-node .",
    "test": "NODE_ENV=test nyc mocha __tests__/**/*.js",
    "clean": "npm i && rimraf dist",
    "backend:build": "babel ./ -d dist/ --ignore ./node_modules,./client/node_modules,./client/package.json,./package.json,./.babelrc,./client/yarn.lock,./client/src,./client/public --copy-files",
    "client:build": "cd client && yarn && yarn build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@hapi/joi": "^16.1.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.7.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "helmet": "^3.21.1",
    "http-errors": "^1.7.3",
    "mocha": "^6.2.1",
    "mockgoose": "^8.0.3",
    "mockgoose-fix": "^7.3.6",
    "mongodb": "^3.3.2",
    "mongodb-memory-server": "^5.2.6",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "node-mocks-http": "^1.8.0",
    "nodemailer": "^6.3.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0"
  }
}
